        <element label="Activities" max_length="40" name="activities" type="journal"/>
        <element label="Cost" max_length="40" name="cost" type="currency"/>
        <element label="Description" max_length="40" name="description" type="html"/>
        <element label="End" max_length="40" name="end" type="glide_date_time"/>
        <element label="Flyer" max_length="40" name="flyer" type="user_image"/>
        <element label="Host" max_length="32" name="host" reference="sys_user" type="reference"/>
